/*圆形大转盘抽奖  */
#box{ 
  width:531rpx;
   height:531rpx; 
   margin:50rpx auto;
   position:relative; 
}
#back{
  width: 100%;
  height: 100%;
}   
#back image{
  display: block;
  width: 100%;
  height: 100%;
}
#zhizhen{ 
    width:170rpx;
    height:220rpx;
    position:absolute;
    left:180.5rpx;
     z-index:999; 
     top:130.5rpx;
    transform-origin:85rpx 135rpx;
}
#zhizhen image{
  display: block;
  width: 100%;
  height: 100%;
}
